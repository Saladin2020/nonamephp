{"NAME":"CARD","LASTUPDATE":"20190718191921","SQLSCRIPT":" SELECT  '[#HOSCODE]' HOSPCODE, RIGHT(CONCAT('000000',p.person_id),6) PID, p.pttype INSTYPE_OLD, pt.pttype_std_code INSTYPE_NEW, REPLACE(REPLACE(p.pttype_no,'\\'',''),'\\\\','') INSID, IF(CAST(p.pttype_begin_date AS UNSIGNED)=0,NULL,CAST(p.pttype_begin_date AS UNSIGNED)) STARTDATE, IF(CAST(p.pttype_expire_date AS UNSIGNED)=0,NULL,CAST(p.pttype_expire_date AS UNSIGNED)) EXPIREDATE, p.pttype_hospmain MAIN, p.pttype_hospsub SUB, CAST(p.last_update AS UNSIGNED) D_UPDATE, p.cid CID  FROM person p   LEFT JOIN pttype t  ON t.pttype = p.pttype   LEFT JOIN provis_instype pt ON pt.code = t.nhso_code   WHERE pt.pttype_std_code is not null or pt.pttype_std_code='' ORDER BY PID"}