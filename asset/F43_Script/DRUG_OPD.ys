{"NAME":"DRUG_OPD","LASTUPDATE":"20190718195845","SQLSCRIPT":" SELECT  '[#HOSCODE]' HOSPCODE, RIGHT(CONCAT('000000',q.pcu_person_id),6) PID, q.seq_id SEQ, CAST(o2.vstdate AS UNSIGNED) DATE_SERV,  sp.provis_code CLINIC, d.did DIDSTD,  concat(d.name,' ',d.strength,' ',d.units) DNAME, SUM(o1.qty) AMOUNT, d.provis_medication_unit_code UNIT, d.units UNIT_PACKING, FORMAT(o1.unitprice,2) DRUGPRICE, FORMAT(o1.cost,2) DRUGCOST, o1.doctor PROVIDER, CAST(q.update_datetime AS UNSIGNED) D_UPDATE, pt.cid CID FROM opitemrece o1   LEFT JOIN ovst o2 ON o2.vn=o1.vn   LEFT JOIN drugitems d ON d.icode=o1.icode   LEFT JOIN ovst_seq q ON q.vn = o2.vn   LEFT JOIN spclty sp ON sp.spclty = o2.spclty  LEFT JOIN patient pt ON pt.hn=o2.hn   WHERE DATE(o2.vstdate)  BETWEEN '[#DATE_START]' and '[#DATE_END]' AND o1.qty>0 AND o1.unitprice>0   AND(d.did IS NOT NULL OR d.did<>'') AND o1.icode LIKE '1%'   GROUP BY o1.vn,o1.icode  ORDER BY DATE_SERV,SEQ"}