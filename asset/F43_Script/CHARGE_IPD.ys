{"NAME":"CHARGE_IPD","LASTUPDATE":"20190718192131","SQLSCRIPT":" SELECT  '[#HOSCODE]' HOSPCODE,  RIGHT(CONCAT('000000',ps.person_id),6) PID,  o.an AN,  CONCAT(DATE_FORMAT(i.regdate,'%Y%m%d'),DATE_FORMAT(i.regtime,'%H%i%s')) DATETIME_ADMIT,  sp.provis_code WARDSTAY,  IFNULL(ie.std_group,'000000') CHARGEITEM,  RIGHT(CONCAT('000000',d.charge_list_id),6) CHARGELIST,  o.qty QUANTITY,  pt.pttype_std_code INSTYPE,  ROUND(IF(o.cost='' OR o.cost IS NULL,'0.00',o.cost),2) COST,  ROUND(IF(o.sum_price='' OR o.sum_price IS NULL,'0.00',o.sum_price),2) PRICE,  ROUND(IF(o.paidst='02','0.00',o.sum_price),2) PAYPRICE,  IFNULL(DATE_FORMAT(o.last_modified,'%Y%m%d%H%i%s'),CONCAT(DATE_FORMAT(o.rxdate,'%Y%m%d'),DATE_FORMAT(o.rxtime,'%H%i%s'))) D_UPDATE, ps.cid CID  FROM opitemrece o  LEFT JOIN ipt i ON i.an = o.an  LEFT JOIN ovst_seq q ON q.vn = i.vn  LEFT JOIN drugitems_charge_list d ON d.icode=o.icode  LEFT JOIN income ie ON ie.income = o.income  LEFT JOIN spclty sp ON sp.spclty = i.spclty  LEFT JOIN pttype py ON py.pttype = i.pttype  LEFT JOIN provis_instype pt ON pt.code = py.nhso_code  LEFT JOIN person ps ON ps.patient_hn=o.hn WHERE o.an IS NOT NULL AND o.an<>'' AND o.qty>0 AND i.dchdate BETWEEN '[#DATE_START]' and '[#DATE_END]' ORDER BY AN,CHARGELIST"}