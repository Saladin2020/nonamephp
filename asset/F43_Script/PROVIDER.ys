{"NAME":"PROVIDER","LASTUPDATE":"20190718221421","SQLSCRIPT":" SELECT  '[#HOSCODE]' HOSPCODE,  d.code PROVIDER, d.licenseno REGISTERNO, d.council_code COUNCIL, d.cid CID, pn.provis_code PRENAME, IF(d.fname='' OR d.fname IS NULL,d.name,d.fname) NAME, d.lname LNAME, d.sex SEX, CAST(d.birth_date AS UNSIGNED) BIRTH, IFNULL(d.provider_type_code,09) PROVIDERTYPE, CAST(d.start_date AS UNSIGNED) STARTDATE, IF(d.finish_date IS NULL, NULL, CAST(d.finish_date AS UNSIGNED)) OUTDATE, d.move_from_hospcode MOVEFROM, d.move_to_hospcode MOVETO, CAST(d.update_datetime AS UNSIGNED) D_UPDATE FROM doctor d LEFT JOIN pname pn ON pn.name  =  d.pname WHERE d.cid is not null  AND (d.start_date BETWEEN '[#DATE_START]' and '[#DATE_END]' or d.finish_date BETWEEN '[#DATE_START]' and '[#DATE_END]') ORDER BY PROVIDER,d.update_datetime"}