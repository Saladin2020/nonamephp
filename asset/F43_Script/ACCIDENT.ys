{"NAME":"ACCIDENT","LASTUPDATE":"20190719112444","SQLSCRIPT":"SELECT '[#HOSCODE]' HOSPCODE,RIGHT(CONCAT('000000',q.pcu_person_id),6) PID,q.seq_id SEQ,CONCAT(DATE_FORMAT(o.vstdate,'%Y%m%d'),DATE_FORMAT(o.vsttime,'%H%i%s')) DATETIME_SERV,DATE_FORMAT(r.arrive_time,'%Y%m%d%H%i%s') DATETIME_AE,RIGHT(CONCAT('00',r.er_accident_type_id),2) AETYPE ,IFNULL(RIGHT(CONCAT('0',r.accident_place_type_id),2),99) AEPLACE,r.visit_type TYPEIN_AE,IF(p.export_code IS NULL OR p.export_code='',IFNULL(r.accident_person_type_id,9),p.export_code ) TRAFFIC,IF(a.export_code IS NULL OR a.export_code='',IFNULL(RIGHT(CONCAT('0',r.accident_transport_type_id),2),99),a.export_code) VEHICLE,IFNULL(r.accident_alcohol_type_id,9) ALCOHOL,IFNULL(r.accident_drug_type_id,9) NACROTIC_DRUG,IFNULL(r.accident_belt_type_id,9) BELT,IFNULL(r.accident_helmet_type_id,9) HELMET,IFNULL(r.accident_airway_type_id,3) AIRWAY,IFNULL(r.accident_bleed_type_id,3) STOPBLEED,IFNULL(r.accident_splint_type_id,3) SPLINT,IFNULL(r.accident_fluid_type_id,3) FLUID,IF(e.export_code IS NULL OR e.export_code='',IFNULL(r.er_emergency_type,5),e.export_code ) URGENCY,FORMAT(r.gcs_e,0) COMA_EYE,FORMAT(r.gcs_v,0) COMA_SPEAK,FORMAT(r.gcs_m,0) COMA_MOVEMENT,DATE_FORMAT(q.update_datetime,'%Y%m%d%H%i%s')  AS D_UPDATE,pt.cid CID FROM er_nursing_detail r  LEFT OUTER JOIN ovst o ON o.vn=r.vn  LEFT OUTER JOIN er_regist er ON er.vn=r.vn  LEFT OUTER JOIN spclty sp ON sp.spclty = o.spclty  LEFT OUTER JOIN ovst_seq q ON q.vn = o.vn  LEFT OUTER JOIN accident_transport_type a ON a.accident_transport_type_id = r.accident_transport_type_id  LEFT OUTER JOIN accident_person_type p ON p.accident_person_type_id = r.accident_person_type_id LEFT OUTER JOIN accident_place_type place ON place.accident_place_type_id = r.accident_place_type_id LEFT OUTER JOIN er_emergency_type e ON e.er_emergency_type = r.er_emergency_type LEFT OUTER JOIN patient pt on pt.hn=o.hn   WHERE r.er_accident_type_id between '1' and '19' AND o.vstdate between '[#DATE_START]' and '[#DATE_END]'  order by o.vn;"}