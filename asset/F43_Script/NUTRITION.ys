{"NAME":"NUTRITION","LASTUPDATE":"20190718220443","SQLSCRIPT":" SELECT  '[#HOSCODE]' HOSPCODE, RIGHT(CONCAT('000000',p3.person_id),6) PID, p1.person_epi_nutrition_id SEQ, CAST(p1.nutrition_date AS UNSIGNED) DATE_SERV, IF(p1.nutrition_hospcode='' OR p1.nutrition_hospcode IS NULL,'[#HOSCODE]',p1.nutrition_hospcode) NUTRITIONPLACE, FORMAT(p1.body_weight,1) WEIGHT, FORMAT(p1.height,0) HEIGHT, FORMAT(p1.head_circum_cm,0) HEADCIRCUM, p1.person_nutrition_childdevelop_type_id CHILDDEVELOP, p1.person_nutrition_food_type_id FOOD, p1.person_nutrition_bottle_type_id BOTTLE, p1.doctor_code PROVIDER, CAST(p1.update_datetime AS UNSIGNED) D_UPDATE, p3.cid CID  FROM person_epi_nutrition p1  LEFT JOIN person_epi p2 ON p1.person_epi_id = p2.person_epi_id  LEFT JOIN person p3 ON p2.person_id = p3.person_id   WHERE timestampdiff(month,p3.birthdate,p1.nutrition_date) BETWEEN 0 AND 216 AND p1.height>0 AND p1.body_weight>0 and p3.cid not like \"011437%\" AND DATE(p1.nutrition_date) BETWEEN '[#DATE_START]' AND '[#DATE_END]'  UNION  SELECT  '[#HOSCODE]' HOSPCODE, RIGHT(CONCAT('000000',p1.person_id),6) PID, v1.village_student_screen_id SEQ, CAST(v1.screen_date AS UNSIGNED) DATE_SERV, IF(v1.screen_hospcode='' OR v1.screen_hospcode IS NULL,'[#HOSCODE]',v1.screen_hospcode) NUTRITIONPLACE, FORMAT(v1.body_weight,1) WEIGHT, FORMAT(v1.height,0) HEIGHT, FORMAT(v1.head_circum_cm,0) HEADCIRCUM, v1.person_nutrition_childdevelop_type_id CHILDDEVELOP, v1.person_nutrition_food_type_id FOOD, v1.person_nutrition_bottle_type_id BOTTLE, v1.doctor_code PROVIDER, CAST(v1.update_datetime AS UNSIGNED) D_UPDATE, p1.cid CID  FROM village_student_screen v1  LEFT JOIN village_student v2 ON v1.village_student_id = v2.village_student_id  LEFT JOIN person p1 ON v2.person_id = p1.person_id  WHERE timestampdiff(month,p1.birthdate,v1.screen_date) BETWEEN 0 AND 216 AND v1.height>0 AND v1.body_weight>0 and p1.cid not like \"011437%\" AND DATE(v1.screen_date) BETWEEN '[#DATE_START]' AND '[#DATE_END]'  UNION  SELECT  '[#HOSCODE]' HOSPCODE, RIGHT(CONCAT('000000',p3.person_id),6) PID, p1.person_wbc_nutrition_id SEQ, CAST(p1.nutrition_date AS UNSIGNED) DATE_SERV, IF(p1.nutrition_hospcode='' OR p1.nutrition_hospcode IS NULL,'[#HOSCODE]',p1.nutrition_hospcode) NUTRITIONPLACE, FORMAT(p1.body_weight,1) WEIGHT, FORMAT(p1.height,0) HEIGHT, FORMAT(p1.head_circum_cm,0) HEADCIRCUM, p1.person_nutrition_childdevelop_type_id CHILDDEVELOP, p1.person_nutrition_food_type_id FOOD, p1.person_nutrition_bottle_type_id BOTTLE, p1.doctor_code PROVIDER, CAST(p1.update_datetime AS UNSIGNED) D_UPDATE, p3.cid CID  FROM person_wbc_nutrition p1  LEFT JOIN person_wbc p2 ON p1.person_wbc_id = p2.person_wbc_id  LEFT JOIN person p3 ON p2.person_id = p3.person_id   WHERE timestampdiff(month,p3.birthdate,p1.nutrition_date) BETWEEN 0 AND 216 AND p1.height>0 AND p1.body_weight>0 and p3.cid not like \"011437%\" AND DATE(p1.nutrition_date) BETWEEN '[#DATE_START]' AND '[#DATE_END]' ORDER BY DATE_SERV"}