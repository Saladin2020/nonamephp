{"NAME":"CHARGE_OPD","LASTUPDATE":"20190718192353","SQLSCRIPT":" SELECT '[#HOSCODE]' HOSPCODE, RIGHT(CONCAT('000000',ps.person_id),6) PID, q.seq_id SEQ, CAST(o2.vstdate AS UNSIGNED) DATE_SERV, sp.provis_code CLINIC, i.std_group CHARGEITEM, RIGHT(CONCAT('000000',d.charge_list_id),6) CHARGELIST, o1.qty QUANTITY, pts.pttype_std_code INSTYPE, FORMAT(o1.cost,2) COST, FORMAT(o1.sum_price,2) PRICE,  FORMAT(IF(o1.paidst='02','0.00',o1.sum_price),2) PAYPRICE, CAST(q.update_datetime AS UNSIGNED) D_UPDATE, pt.cid CID FROM opitemrece o1 LEFT JOIN drugitems_charge_list d ON d.icode=o1.icode LEFT JOIN income i ON o1.income=i.income LEFT JOIN ovst o2 ON o2.vn=o1.vn LEFT JOIN vn_stat v ON v.vn=o1.vn LEFT JOIN pttype py  ON py.pttype = o2.pttype LEFT JOIN provis_instype pts ON pts.code = py.nhso_code LEFT JOIN ovst_seq q ON q.vn = o2.vn LEFT JOIN spclty sp ON sp.spclty = o2.spclty LEFT JOIN patient pt on pt.hn=o1.hn LEFT JOIN person ps on ps.patient_hn=pt.hn WHERE 1=1 AND o1.qty>0 AND o2.vstdate BETWEEN '[#DATE_START]' and '[#DATE_END]' AND (o1.an is null OR o1.an='')  ORDER BY DATE_SERV,CHARGELIST"}