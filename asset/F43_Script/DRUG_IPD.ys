{"NAME":"DRUG_IPD","LASTUPDATE":"20190718195700","SQLSCRIPT":" SELECT  '[#HOSCODE]' HOSPCODE, RIGHT(CONCAT('000000',q.pcu_person_id),6) PID, o.an AN, CONCAT(DATE_FORMAT(i.regdate,'%Y%m%d'),DATE_FORMAT(i.regtime,'%H%i%s')) DATETIME_ADMIT, sp.provis_code WARDSTAY, IF(o.item_type='H',2,1) TYPEDRUG, d.did DIDSTD,  concat(d.name,' ',d.strength,' ',d.units) DNAME, CAST(MIN(o.rxdate) AS UNSIGNED) DATESTART, CAST(MAX(o.rxdate) AS UNSIGNED) DATEFINISH, SUM(o.qty) AMOUNT, d.provis_medication_unit_code UNIT, d.units UNIT_PACKING, FORMAT(o.unitprice,2) DRUGPRICE, FORMAT(o.cost,2) DRUGCOST, IF(o.doctor,o.doctor,dr.code) PROVIDER, IF(o.last_modified='' OR o.last_modified IS NULL,CONCAT(CAST(o.rxdate AS UNSIGNED),DATE_FORMAT(o.rxtime,'%H%i%s')),CAST(o.last_modified AS UNSIGNED)) D_UPDATE, pt.cid CID FROM opitemrece o   LEFT JOIN drugitems d ON d.icode = o.icode   LEFT JOIN ipt i ON i.an = o.an   LEFT JOIN ovst_seq q ON q.vn = i.vn   LEFT JOIN spclty sp ON sp.spclty = i.spclty   LEFT JOIN opduser u on u.loginname = o.staff LEFT JOIN doctor dr on dr.code = u.doctorcode LEFT JOIN patient pt on pt.hn=o.hn WHERE DATE(i.dchdate) BETWEEN '[#DATE_START]' and '[#DATE_END]' AND o.an<>'' AND o.qty>0 AND o.unitprice>0  AND o.icode LIKE '1%' AND(d.did IS NOT NULL OR d.did<>'') AND o.an IS NOT NULL  GROUP BY o.an, o.icode ORDER BY AN,DATESTART"}