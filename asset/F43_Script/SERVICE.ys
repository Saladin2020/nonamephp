{"NAME":"SERVICE","LASTUPDATE":"20190718160920","SQLSCRIPT":" SELECT  '[#HOSCODE]' HOSPCODE,  RIGHT(CONCAT('000000',p.person_id),6) PID, pt.hn HN, q.seq_id SEQ, CAST(o.vstdate AS UNSIGNED) DATE_SERV, DATE_FORMAT(o.vsttime,'%H%i%s') TIME_SERV, IF(vi.village_moo=0,2,1)LOCATION, IF(o.visit_type='I',1,2) INTIME, pts.pttype_std_code INSTYPE, REPLACE(REPLACE(o.pttypeno,'\\'',''),'\\\\','')  INSID, o.hospmain MAIN, IF(o.ovstist='' OR o.ovstist IS NULL,'1',CAST(oi.export_code AS UNSIGNED)) TYPEIN, IF(rf.refer_hospcode='' OR rf.refer_hospcode IS NULL,rf.hospcode,CAST(rf.refer_hospcode AS UNSIGNED)) REFERINHOSP, IF(rf.refer_hospcode IS NULL,'',IF(rfi.export_code,rfi.export_code,1)) CAUSEIN, CONVERT(REPLACE(REPLACE(substring_index(substring_index(s.cc, '|', -1),'|', 1) ,'\\'',''),'\\\\','') USING utf8)  CHIEFCOMP, IF(hm.in_hospcode_service IS NULL OR hm.in_hospcode_service IN ('Y',''),1,2) SERVPLACE, s.temperature BTEMP, s.bps SBP, s.bpd DBP, s.pulse PR, s.rr RR,  IF(o.ovstost='' OR o.ovstost IS NULL,'1',CAST(oo.export_code AS UNSIGNED)) TYPEOUT, IF(ro.refer_hospcode='' OR ro.refer_hospcode IS NULL,ro.hospcode,CAST(ro.refer_hospcode AS UNSIGNED)) REFEROUTHOSP, IF(ro.refer_hospcode IS NULL,'',IF(rfo.export_code,rfo.export_code,1))CAUSEOUT, IF(q.service_cost=0,FORMAT(IFNULL(v.item_money,0),2),FORMAT(IFNULL(q.service_cost,0),2)) COST, FORMAT(IFNULL(v.income,0),2) PRICE, FORMAT(IFNULL(v.paid_money,0),2) PAYPRICE, FORMAT(IFNULL(v.paid_money,0),2) ACTUALPAY, CAST(q.update_datetime AS UNSIGNED) D_UPDATE, o.hospsub HSUB, pt.cid CID FROM ovst o   LEFT JOIN vn_stat v ON v.vn=o.vn   LEFT JOIN opdscreen s ON s.vn=o.vn   LEFT JOIN doctor d ON d.code=v.dx_doctor   LEFT JOIN ovst_seq q ON q.vn = o.vn   LEFT JOIN patient pt ON pt.hn = o.hn   LEFT JOIN person p ON p.patient_hn = pt.hn LEFT JOIN house h ON h.house_id = p.house_id  LEFT JOIN village vi ON vi.village_id = h.village_id  LEFT JOIN pttype y ON y.pttype = o.pttype   LEFT JOIN provis_instype pts ON pts.code = y.nhso_code   LEFT JOIN referin rf ON rf.vn = o.vn   LEFT JOIN referout ro ON ro.vn = o.vn   LEFT JOIN health_med_service hm ON hm.vn = o.vn   LEFT JOIN spclty sp ON sp.spclty = o.spclty  LEFT JOIN rfrcs rfi ON rfi.rfrcs = rf.rfrcs  LEFT JOIN rfrcs rfo ON rfo.rfrcs = ro.rfrcs  LEFT JOIN ovstist oi ON oi.ovstist=o.ovstist  LEFT JOIN ovstost oo ON oo.ovstost=o.ovstost  WHERE o.vstdate BETWEEN '[#DATE_START]' and '[#DATE_END]' order by DATE_SERV"}