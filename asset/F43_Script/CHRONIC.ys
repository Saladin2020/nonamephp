{"NAME":"CHRONIC","LASTUPDATE":"20190718192553","SQLSCRIPT":" SELECT * FROM (SELECT  '[#HOSCODE]' HOSPCODE, RIGHT(CONCAT('000000',p.person_id),6) PID, IFNULL(CAST(p1.regdate AS UNSIGNED),CAST(p1.lastvisit AS UNSIGNED)) DATE_DIAG, UPPER(IFNULL((SELECT icd10 FROM clinic_persist_icd WHERE hn = p1.hn AND clinic = p1.clinic AND LEFT(icd10,3) = LEFT(c.icd10,3) LIMIT 1),c.icd10)) CHRONIC,  '' HOSP_DX, '' HOSP_RX, DATE_FORMAT(p1.dchdate ,'%Y%m%d') DATE_DISCH,  IFNULL((SELECT RIGHT(CONCAT('0',provis_typedis),2) from clinic_member_status  where clinic_member_status_id = p1.clinic_member_status_id LIMIT 1),'04') TYPEDISCH,  CAST(p1.lastupdate AS UNSIGNED) D_UPDATE, p2.cid CID FROM clinicmember p1   LEFT JOIN patient p2 on p2.hn = p1.hn   LEFT JOIN ptcardno pc on pc.hn = p1.hn and pc.cardtype = '01'  LEFT JOIN person p on p.cid = REPLACE(pc.cardno,'-','')   LEFT JOIN clinic c on c.clinic = p1.clinic   WHERE p1.clinic IN (SELECT clinic FROM clinic WHERE  (no_export is null OR no_export<>'Y' OR no_export=''))   UNION SELECT  '[#HOSCODE]' HOSPCODE, RIGHT(CONCAT('000000',p.person_id),6) PID, IFNULL(CAST(p1.regdate AS UNSIGNED),CAST(DATE(p1.last_update) AS UNSIGNED)) DATE_DIAG, UPPER(c.icd10) CHRONIC,  '' HOSP_DX, '' HOSP_RX, CAST(p1.discharge_date AS UNSIGNED) DATE_DISCH,  IFNULL((SELECT RIGHT(CONCAT('0',provis_typedis),2) from clinic_member_status  WHERE clinic_member_status_id = p1.clinic_member_status_id LIMIT 1),'04') TYPEDISCH, CAST(p1.last_update AS UNSIGNED) D_UPDATE, p.cid CID FROM person_chronic p1  LEFT JOIN person p ON p1.person_id = p.person_id AND p.cid NOT IN ('-999') LEFT JOIN clinic c ON c.clinic = p1.clinic   WHERE  p1.clinic IN (SELECT clinic FROM clinic WHERE  (no_export is null OR no_export<>'Y' OR no_export='')))C GROUP BY PID,CHRONIC "}