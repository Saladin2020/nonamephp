{"NAME":"EPI","LASTUPDATE":"20190718203757","SQLSCRIPT":" SELECT  '[#HOSCODE]' HOSPCODE, RIGHT(CONCAT('000000',q.pcu_person_id),6) PID, q.seq_id SEQ, CAST(p2.anc_service_date AS UNSIGNED) DATE_SERV,  '106' VACCINETYPE, '[#HOSCODE]' VACCINEPLACE, p1.anc_doctor_code PROVIDER, CAST(p3.last_update AS UNSIGNED) D_UPDATE, ps.cid CID FROM person_anc_service_detail p1  LEFT JOIN anc_service a1 ON p1.anc_service_id = a1.anc_service_id   LEFT JOIN person_anc_service p2 ON p1.person_anc_service_id = p2.person_anc_service_id  LEFT JOIN person_anc p3 ON p2.person_anc_id = p3.person_anc_id   LEFT JOIN ovst_seq q ON q.vn = p2.vn  LEFT JOIN person ps ON ps.person_id=p3.person_id WHERE q.pcu_person_id IS NOT NULL and  p2.anc_service_date BETWEEN '[#DATE_START]' and '[#DATE_END]' UNION  SELECT  '[#HOSCODE]' HOSPCODE, RIGHT(CONCAT('000000',q.pcu_person_id),6) PID, q.seq_id SEQ, CAST(w3.service_date AS UNSIGNED) DATE_SERV,  w2.export_vaccine_code VACCINETYPE, '[#HOSCODE]' VACCINEPLACE, w1.doctor_code PROVIDER, CAST(w4.last_update AS UNSIGNED) D_UPDATE, ps.cid CID FROM person_wbc_vaccine_detail w1  LEFT JOIN wbc_vaccine w2 ON w2.wbc_vaccine_id = w1.wbc_vaccine_id LEFT JOIN person_wbc_service w3 ON w3.person_wbc_service_id = w1.person_wbc_service_id    LEFT JOIN person_wbc w4 ON w4.person_wbc_id = w3.person_wbc_id   LEFT JOIN ovst_seq q ON w3.vn = q.vn  LEFT JOIN person ps on ps.person_id=w4.person_id   WHERE q.pcu_person_id IS NOT NULL and w3.service_date BETWEEN '[#DATE_START]' and '[#DATE_END]' UNION  SELECT   '[#HOSCODE]' HOSPCODE, RIGHT(CONCAT('000000',q.pcu_person_id),6) PID, q.seq_id SEQ, CAST(v3.vaccine_date AS UNSIGNED) DATE_SERV,  v2.export_vaccine_code VACCINETYPE, '[#HOSCODE]' VACCINEPLACE, v1.doctor_code PROVIDER, CAST(v4.last_update AS UNSIGNED) D_UPDATE, ps.cid CID FROM village_student_vaccine_list v1     INNER JOIN student_vaccine v2 ON v2.student_vaccine_id = v1.student_vaccine_id  LEFT JOIN village_student_vaccine v3 ON v3.village_student_vaccine_id = v1.village_student_vaccine_id  LEFT JOIN village_student v4 ON  v4.village_student_id = v3.village_student_id  INNER JOIN ovst_seq q ON q.vn = v3.vn LEFT JOIN person ps on ps.person_id=v4.person_id   where v3.vaccine_date BETWEEN '[#DATE_START]' and '[#DATE_END]' UNION  SELECT '[#HOSCODE]' HOSPCODE, RIGHT(CONCAT('000000',q.pcu_person_id),6) PID, q.seq_id SEQ, CAST(e3.vaccine_date AS UNSIGNED) DATE_SERV,  e1.export_vaccine_code VACCINETYPE, '[#HOSCODE]' VACCINEPLACE, e2.doctor_code PROVIDER, CAST(e4.last_update AS UNSIGNED) D_UPDATE, ps.cid CID FROM epi_vaccine e1  LEFT JOIN person_epi_vaccine_list e2 ON e1.epi_vaccine_id = e2.epi_vaccine_id   LEFT JOIN person_epi_vaccine e3 ON e2.person_epi_vaccine_id = e3.person_epi_vaccine_id   LEFT JOIN person_epi e4 ON e3.person_epi_id = e4.person_epi_id   INNER JOIN ovst_seq q ON e3.vn = q.vn LEFT JOIN person ps on ps.person_id=e4.person_id    WHERE 1=1 and e3.vaccine_date BETWEEN '[#DATE_START]' and '[#DATE_END]' UNION  SELECT  '[#HOSCODE]' HOSPCODE, RIGHT(CONCAT('000000',q.pcu_person_id),6) PID, q.seq_id SEQ, CAST(o.vstdate AS UNSIGNED) DATE_SERV,  p.export_vaccine_code VACCINETYPE, '[#HOSCODE]' VACCINEPLACE, o.doctor PROVIDER, CAST(q.update_datetime AS UNSIGNED) D_UPDATE, pt.cid CID  FROM ovst_vaccine v  LEFT JOIN ovst o ON o.vn = v.vn LEFT JOIN person_vaccine p ON v.person_vaccine_id = p.person_vaccine_id LEFT JOIN ovst_seq q ON o.vn = q.vn LEFT JOIN patient pt on pt.hn=o.hn WHERE q.pcu_person_id IS NOT NULL and p.export_vaccine_code is not null and o.vstdate BETWEEN '[#DATE_START]' and '[#DATE_END]' UNION  SELECT  '[#HOSCODE]' HOSPCODE, RIGHT(CONCAT('000000',p1.person_id),6) PID, '' SEQ, CAST(p1.vaccine_date AS UNSIGNED) DATE_SERV,  p2.export_vaccine_code VACCINETYPE, IF(p1.hospcode='' OR p1.hospcode IS NULL,'[#HOSCODE]',p1.hospcode)  VACCINEPLACE, '' PROVIDER, CAST(p1.update_datetime AS UNSIGNED) D_UPDATE, p3.cid CID FROM person_vaccine_elsewhere p1   LEFT JOIN person_vaccine p2 ON p2.person_vaccine_id = p1.person_vaccine_id LEFT JOIN person p3 ON p3.person_id = p1.person_id WHERE 1=1 and p1.vaccine_date BETWEEN '[#DATE_START]' and '[#DATE_END]' ORDER BY DATE_SERV,VACCINETYPE,PID"}