{"NAME":"SPECIALPP","LASTUPDATE":"20190718221959","SQLSCRIPT":" SELECT  '[#HOSCODE]' HOSPCODE,  RIGHT(CONCAT('000000',s.pcu_person_id),6) PID, s.seq_id SEQ,  CAST(v.vstdate AS UNSIGNED) DATE_SERV,  d.pp_special_service_place_type_id SERVPLACE,  t.pp_special_code PPSPECIAL,  d.dest_hospcode PPSPLACE,  d.doctor PROVIDER,  CAST(d.entry_datetime AS UNSIGNED) D_UPDATE, pt.cid CID   FROM pp_special d  LEFT JOIN ovst v ON d.vn = v.vn LEFT JOIN ovst_seq s ON v.vn = s.vn LEFT JOIN pp_special_type t ON d.pp_special_type_id = t.pp_special_type_id LEFT JOIN patient pt on pt.hn=v.hn WHERE 1=1  AND DATE(v.vstdate) BETWEEN '[#DATE_START]' and '[#DATE_END]' GROUP BY PID,DATE_SERV,PPSPECIAL order by DATE_SERV,PID,SEQ"}